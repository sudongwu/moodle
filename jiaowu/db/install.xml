<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="enrol/jiaowu/db" VERSION="20200905" COMMENT="XMLDB file for Moodle enrol/jiaowu"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="enrol_jiaowu" COMMENT="Default comment for enrol_jiaowu, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="enrolid" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="报名方式id"/>
        <FIELD NAME="termid" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="学期id"/>
        <FIELD NAME="collegename" TYPE="char" LENGTH="200" NOTNULL="false" SEQUENCE="false" COMMENT="开课学院名称"/>
        <FIELD NAME="courseid" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="教务系统上课程的id"/>
        <FIELD NAME="coursename" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="课程名程"/>
        <FIELD NAME="stunum" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="选课人数"/>
        <FIELD NAME="data" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="存放同步信息"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="同步时间"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" COMMENT="修改时间"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>